{
  "name": "atelier",
  "version": "1.0.0",
  "description": "",
  "main": "config.js",
  "scripts": {
    "test:related": "jest --coverage --testPathPattern=tests/client/src/components/related --testResultsProcessor=jest-sonar-reporter --watch",
    "test:QA": "jest --coverage --testPathPattern=tests/client/src/components/QA --testResultsProcessor=jest-sonar-reporter",
    "test:RR": "jest --coverage --testPathPattern=tests/client/src/components/ratingsReviews --testResultsProcessor=jest-sonar-reporter",
    "test:overview": "jest --coverage --testPathPattern=tests/client/src/components/overview --testResultsProcessor=jest-sonar-reporter --watch",
    "test": "jest --coverage --testResultsProcessor=jest-sonar-reporter",
    "test-updateSnapshot": "jest --updateSnapshot --coverage --testResultsProcessor=jest-sonar-reporter",
    "test-init": "jest --init",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls",
    "start": "nodemon server.js",
    "build": "webpack --mode development --watch",
    "dev": "npm run build & npm run start & npm run test",
    "production-build": "webpack --mode production",
    "production-server": "node server.js",
    "production-app": "npm run production-build & npm run production-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpp29-fec-gouda/atelier.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rpp29-fec-gouda/atelier/issues"
  },
  "homepage": "https://github.com/rpp29-fec-gouda/atelier#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.0.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "coveralls": "^3.1.1",
    "css-loader": "^6.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "jest-css-modules-transform": "^4.3.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.12",
    "style-loader": "^3.1.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "imgbb-uploader": "^1.3.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "jest": {
    "^react(.*)$": "<rootDir>/vendor/react-master$1",
    "^config$": "<rootDir>/configs/app-config.js"
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "testResults",
    "reportFile": "sonar-report.xml",
    "indent": 4
  }
}
